<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet id="01-05-2021 create initial tables" author="Joosep">
        <createTable tableName="flights">
            <column name="row_id" type="text">
                <constraints primaryKey="true"/>
            </column>
            <column name="id" type="text"/>
            <column name="validUntil" type="text"/>
            <column name="legs_id" type="text"/>
            <column name="legs_routeInfo_id" type="text"/>
            <column name="legs_routeInfo_from_id" type="text"/>
            <column name="legs_routeInfo_from_name" type="text"/>
            <column name="legs_routeInfo_to_id" type="text"/>
            <column name="legs_routeInfo_to_name" type="text"/>
            <column name="legs_routeInfo_distance" type="bigint"/>
            <column name="legs_providers_id" type="text"/>
            <column name="legs_providers_company_id" type="text"/>
            <column name="legs_providers_company_name" type="text"/>
            <column name="legs_providers_price" type="number"/>
            <column name="legs_providers_flightStart" type="text"/>
            <column name="legs_providers_flightEnd" type="text"/>
        </createTable>

    </changeSet>
</databaseChangeLog>
